{
  "name": "nuxt-feather-icons",
  "version": "1.1.2",
  "description": "Nuxt 3 Feather icons",
  "repository": "4sllan/nuxt-feather-icons",
  "author": "Aslan Gama <@4slan>",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "build": "nuxi build",
    "lint": "eslint . --max-warnings=0",
    "clean": "rm -rf scripts/.nuxt dist .nuxt",
    "typecheck": "nuxi prepare scripts && tsc --noEmit",
    "typecheck:refresh": "nuxi prepare scripts && tsc --noEmit",
    "dev:prepare": "nuxt-module-build --stub"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.5.4",
    "@nuxt/schema": "^3.8.2",
    "nuxt": "^3.8.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "feather-icons": "^4.29.1",
    "fs-extra": "^11.2.0",
    "pascalcase": "^2.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
